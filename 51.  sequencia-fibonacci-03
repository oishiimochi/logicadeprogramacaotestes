algoritmo "SequenciaFibonacci03"
var
   C, T1, T2, T3: inteiro
Funcao ProximoFibonacci (var A, B: inteiro): inteiro
var C: inteiro
inicio
   C := A + B
   A := B
   B := C
   Retorne C
FimFuncao
inicio
   T1 <- 0
   EscrevaL (T1)
   T2 <- 1
   EscrevaL (T2)
   Para C := 3 ate 10 faca
      T3 <- ProximoFibonacci (T1, T2)
      EscrevaL (T3)
   FimPara
fimalgoritmo
