algoritmo "FatorialFuncao"
var
   N, F: Inteiro

Funcao Fatorial(V: inteiro): inteiro
var C, R: inteiro
inicio
   R <- 1
   Para C <- 1 ate V faca
      R <- R * C
   FimPara
   Retorne R
FimFuncao
inicio
   Escreva ("Digite um número: ")
   Leia (N)
   F <- Fatorial(N)
   Escreva ("O valor de", N, "! fatorial é ", F)
fimalgoritmo
